// C++ code
//
void setup()
{
  pinMode(LED_BUILTIN, OUTPUT);
}

void loop()
{
  digitalWrite(3, HIGH);
  delay(500); // Wait for 1000 millisecond(s)
  digitalWrite(3, LOW);
  delay(500); // Wait for 1000 millisecond(s)
}

